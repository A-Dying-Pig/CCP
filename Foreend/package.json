{
  "name": "hello-world",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --open",
    "build": "vue-cli-service build --mode multi-page --no-clean",
    "lint": "vue-cli-service lint",
    "analyz": "NODE_ENV=production npm_config_report=true npm run build"
  },
  "dependencies": {
    "debug": "^3.1.0",
    "filestack-js": "^0.11.4",
    "lodash": "^4.17.10",
    "pdfjs-dist": "^2.0.489",
    "ajv": "^6.5.5",
    "ajv-keywords": "^3.2.0",
    "sl-vue-tree": "^1.8.2",
    "vue": "^2.5.17",
    "vue-resource": "^1.5.1",
    "vue-router": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.1.1",
    "@vue/cli-plugin-eslint": "^3.1.5",
    "@vue/cli-service": "^3.1.4",
    "axios": "^0.18.0",
    "babel-eslint": "^10.0.1",
    "element-ui": "^2.4.9",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0-0",
    "v-distpicker": "^1.0.20",
    "vue-pdf": "^3.3.1",
    "vue-simditor": "0.0.3",
    "vue-svg-loader": "^0.5.0",
    "vue-template-compiler": "^2.5.17",
    "vue-ztree": "^1.0.0",
    "webpack-bundle-analyzer": "^3.0.3"
  },
  "babel": {
    "presets": [
      "@vue/app"
    ]
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "off",
      "no-unused-vars": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "browser": {
    "vue": "vue/dist/vue.common.js"
  },
  "alias": {
    "vue": "./node_modules/vue/dist/vue.common.js"
  }
}
