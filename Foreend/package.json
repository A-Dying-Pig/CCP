{
  "name": "hello-world",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --open",
    "build": "vue-cli-service build --mode multi-page --no-clean",
    "lint": "vue-cli-service lint",
    "analyz": "NODE_ENV=production npm_config_report=true npm run build",
    "test:unit": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text mocha-webpack NODE_ENV=test --webpack-config webpack.test.conf.js --require tests/setup.js tests/unit/*.spec.js --require ignore-styles",
    "test:help": "mocha-webpack --help"
  },
  "dependencies": {
    "ajv": "^6.5.5",
    "ajv-keywords": "^3.2.0",
    "compression-webpack-plugin": "^2.0.0",
    "debug": "^3.1.0",
    "filestack-js": "^0.11.4",
    "lodash": "^4.17.10",
    "pdfjs-dist": "^2.0.489",
    "sl-vue-tree": "^1.8.2",
    "vue": "^2.5.17",
    "vue-resource": "^1.5.1",
    "vue-router": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.1.1",
    "@vue/cli-plugin-eslint": "^3.1.5",
    "@vue/cli-service": "^3.1.4",
    "@vue/test-utils": "^1.0.0-beta.27",
    "axios": "^0.18.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-istanbul": "^5.1.0",
    "babel-register": "^6.26.0",
    "chai": "^4.2.0",
    "cross-env": "^5.2.0",
    "element-ui": "^2.4.9",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0-0",
    "expect": "^23.6.0",
    "extract-text-webpack-plugin": "^4.0.0-alpha.0",
    "html-webpack-plugin": "^3.2.0",
    "ignore-styles": "^5.0.1",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jsdom": "^13.1.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^5.2.0",
    "mocha-webpack": "2.0.0-beta.0",
    "moxios": "^0.4.0",
    "null-loader": "^0.1.1",
    "nyc": "^13.1.0",
    "v-distpicker": "^1.0.20",
    "vue-pdf": "^3.3.1",
    "vue-simditor": "0.0.3",
    "vue-svg-loader": "^0.5.0",
    "vue-template-compiler": "^2.5.17",
    "vue-ztree": "^1.0.0",
    "webpack": "^4.27.1",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-node-externals": "^1.7.2",
    "worker-loader": "^2.0.0"
  },
  "babel": {
    "presets": [
      "@vue/app"
    ]
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "off",
      "no-unused-vars": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "nyc": {
    "include": [
      "src/components/*.(js|vue)",
      "src/pages/*.(js|vue)"
    ],
    "instrument": false,
    "sourceMap": false,
    "check-coverage": true,
    "per-file": true,
    "lines": 90,
    "statements": 90,
    "functions": 90,
    "branches": 90,
    "reporter": [
      "lcov",
      "text",
      "text-summary"
    ],
    "extension": [
      ".js"
    ],
    "cache": true,
    "all": true
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "browser": {
    "vue": "vue/dist/vue.common.js"
  },
  "alias": {
    "vue": "./node_modules/vue/dist/vue.common.js"
  }
}
